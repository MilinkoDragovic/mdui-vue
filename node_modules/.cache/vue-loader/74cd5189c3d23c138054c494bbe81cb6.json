n</template>\n\n<script lang=\"ts\">\nimport {defineComponent} from \"vue\";\n\nexport default defineComponent({\n    name: \"MdInputText\",\n    props: {\n        modelValue: null\n    },\n    methods: {\n        onInputChange(event: any) {\n            const target = (<HTMLInputElement>event.target);\n            this.$emit(\"update:modelValue\", target.value);\n        }\n    },\n    computed: {\n        isInputFilled(): any {\n            return this.inputValue !== null && this.inputValue.toString().length > 0\n        }\n    }\n});\n</script>\n\n<style lang=\"scss\">\n.md-input-text {\n    margin: 0;\n    padding: .5rem .5rem;\n    border-radius: 0.25rem;\n    font-size: 1rem;\n    color: #495057;\n    background: #ffffff;\n    border: 1px solid #ced4da;\n    transition: background-color 0.2s, color 0.2s, border-color 0.2s, box-shadow 0.2s;\n    appearance: none;\n\n    &:focus {\n        outline: 0 none;\n        outline-offset: 0;\n        box-shadow: 0 0 0 0.2rem #a6d5fa;\n        border-color: #2196F3;\n    }\n}\n\n.md-fluid {\n    .md-input-text {\n        width: 100%;\n    }\n}\n\n/* InputGroup */\n.md-input-group {\n    display: flex;\n    align-items: stretch;\n    width: 100%;\n}\n\n.md-input-group-addon {\n    display: flex;\n    align-items: center;\n    justify-content: center;\n}\n\n.md-input-group .md-float-label {\n    display: flex;\n    align-items: stretch;\n    width: 100%;\n}\n\n.md-input-group .md-input-text,\n.md-fluid .md-input-group .md-input-text,\n.md-input-group .md-inputwrapper,\n.md-fluid .md-input-group .md-input {\n    flex: 1 1 auto;\n    width: 1%;\n}\n\n/* Floating Label */\n.md-float-label {\n    display: block;\n    position: relative;\n}\n\n.md-float-label label {\n    position: absolute;\n    pointer-events: none;\n    top: 50%;\n    margin-top: -.5rem;\n    transition-property: all;\n    transition-timing-function: ease;\n    line-height: 1;\n}\n\n.md-float-label textarea ~ label {\n    top: 1rem;\n}\n\n.md-float-label input:focus ~ label,\n.md-float-label input.md-filled ~ label,\n.md-float-label textarea:focus ~ label,\n.md-float-label textarea.md-filled ~ label,\n.md-float-label .md-inputwrapper-focus ~ label,\n.md-float-label .md-inputwrapper-filled ~ label {\n    top: -.75rem;\n    font-size: 12px;\n}\n\n.md-float-label .input:-webkit-autofill ~ label {\n    top: -20px;\n    font-size: 12px;\n}\n\n.md-input-icon-left,\n.md-input-icon-right {\n    position: relative;\n    display: inline-block;\n}\n\n.md-input-icon-left > i,\n.md-input-icon-right > i {\n    position: absolute;\n    top: 50%;\n    margin-top: -.5rem;\n}\n\n.md-fluid .md-input-icon-left,\n.md-fluid .md-input-icon-right {\n    display: block;\n    width: 100%;\n}\n\n.md-float-label > label {\n    left: 0.5rem;\n    color: #6c757d;\n    transition-duration: 0.2s;\n}\n</style>\n"]}]}